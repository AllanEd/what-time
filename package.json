{
  "name": "what-time",
  "version": "1.0.0",
  "description": "a small meeting app",
  "main": "index.js",
  "scripts": {
    "debug": "nodemon --watch server --exec node --require dotenv/config --require babel-register --inspect server/server.js",
    "start": "node ./server/server.js",
    "build": "babel server/ -d server/",
    "lint": "eslint src/**",
    "test": "mocha test --recursive --compilers js:babel-register",
    "flow": "flow",
    "win-killall": "taskkill /im node.exe /F",
    "mongo-db": "md mongo-db & \"C:/Program Files/MongoDb/Server/3.6/bin/mongod.exe\" --dbpath mongo-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllanEd/what-time.git"
  },
  "keywords": [
    "meeting",
    "app",
    "react",
    "native"
  ],
  "author": "alexander sattelmaier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AllanEd/what-time/issues"
  },
  "homepage": "https://github.com/AllanEd/what-time#readme",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.6",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "validator": "^10.6.0",
    "webpack": "^4.16.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.78.0",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "sinon": "^6.1.5",
    "supertest": "^3.1.0"
  }
}
